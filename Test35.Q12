//Q12 The Magical Staircase
//Number of ways to reach step n with 1 or 2 steps (Fibonacci-like).

public class Q12_The_Magical_Staircase {
    public static long ways(int n){
        if(n==0) return 1;
        if(n==1) return 1;
        long a=1,b=1;
        for(int i=2;i<=n;i++){
            long c=a+b; a=b; b=c;
        }
        return b;
    }
    public static void main(String[] args){
        System.out.println(ways(4)); // 5
    }
}
